{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "quotemark": false,
        "no-empty": false,
        "no-console": false,
        "object-literal-sort-keys": false,
        "member-ordering": false,
        "no-string-literal": false,
        "no-namespace": false,
        "variable-name": false,
        "ordered-imports": false,
        "trailing-comma": false,
        "arrow-parens": false,
        "max-line-length": [
            true,
            "180"
        ]
    },
    "rulesDirectory": []
}